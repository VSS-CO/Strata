// Test 1: Extended Type System
// Test all new primitive types from Rust, Go, TypeScript, Python

// Basic types (original)
let x: int = 42
let y: float = 3.14
let z: bool = true
let c: char = 'A'
let s: string = "hello"
let a: any = 999

// Rust-style signed integers
let i8_val: i8 = 127
let i16_val: i16 = 32767
let i32_val: i32 = 2147483647
let i64_val: i64 = 9223372036854775807

// Rust-style unsigned integers
let u8_val: u8 = 255
let u16_val: u16 = 65535
let u32_val: u32 = 4294967295
let u64_val: u64 = 18446744073709551615

// Float types
let f32_val: f32 = 3.14
let f64_val: f64 = 2.71828

// Collection types
let arr: array = [1, 2, 3]
let lst: list = [4, 5, 6]
let dct: dict = {}
let mp: map = {}
let st: set = {}
let tpl: tuple = (1, 2, 3)

// Advanced types
let opt: option = Some(42)
let res: result = Ok(100)
let prom: promise = Promise()
let vd: void = null

// Regex and patterns
let rgx: regex = /test/
let ptn: pattern = "\\d+"

// Scientific types
let cx: complex = (3+4i)
let mtx: matrix = [[1, 2], [3, 4]]
let df: dataframe = {}

// Functional types
let fn: callable = func(x: int) => int { return x * 2 }
let lmb: lambda = func(a: int) => int { return a + 1 }
let cls: closure = func() => int { return 42 }
