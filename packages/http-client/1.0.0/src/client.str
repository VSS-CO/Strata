// http-client v1.0.0
// HTTP client implementation

import io from str

func get(url: string) => string {
  io.print("GET " + url)
  return ""
}

func post(url: string, body: string) => string {
  io.print("POST " + url)
  return ""
}

func put(url: string, body: string) => string {
  io.print("PUT " + url)
  return ""
}

func delete(url: string) => string {
  io.print("DELETE " + url)
  return ""
}
